@keyframes dialogSlipToUP {
  0% {
    opacity: 0.3;
  }
  100% {
    opacity: 1;
  }
}
.dtm-modal {
  position: fixed;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  z-index: 5000;
  display: flex;
  align-items: center;
  justify-content: center;
  padding: 80rpx;
  background: rgba(0, 0, 0, 0.4);
  background: var(--common_black2_color, rgba(0, 0, 0, 0.4));
  animation: dialogSlipToUP 0.2s ease-out 1 forwards;
}
.dtm-modal-content {
  box-shadow: var(--common_box_shadow_l__box-shadow, 0px 12px 32px rgba(0, 0, 0, 0.24));
  width: 560rpx;
  overflow: hidden;
  text-align: center;
  word-break: break-word;
  background: #ffffff;
  background: var(--common_bg_z1_color, #ffffff);
  border-radius: var(--common_border_radius_l, 8px);
  margin-top: -80rpx;
}
.dtm-modal-dialog {
  text-align: left;
}
.dtm-modal-title {
  font-size: var(--common_body_bold_text_style__font-size, 17px);
  font-weight: bold;
  line-height: var(--common_body_bold_text_style__line-height, 1.5);
  margin-top: 48rpx;
  margin-right: 48rpx;
  margin-left: 48rpx;
  color: #171a1d;
  color: var(--common_level1_base_color, #171a1d);
}
.dtm-modal-title.img-title {
  margin-top: 24rpx;
}
.dtm-modal-img {
  display: flex;
  justify-content: center;
}
.dtm-modal-img-large {
  width: 591rpx;
  height: 312rpx;
}
.dtm-modal-img-small {
  width: 96rpx;
  height: 96rpx;
  margin-top: 48rpx;
}
.dtm-modal-describe {
  font-size: var(--common_action_text_style__font-size, 14px);
  line-height: var(--common_action_text_style__line-height, 1.5);
  margin-top: 24rpx;
  margin-right: 48rpx;
  margin-left: 48rpx;
  color: rgba(23, 26, 29, 0.6);
  color: var(--common_level2_base_color, rgba(23, 26, 29, 0.6));
}
.dtm-modal-describe-mtop {
  margin-top: 40rpx;
  color: #171a1d;
  color: var(--common_level1_base_color, #171a1d);
}
.dtm-modal-input-wrap {
  position: relative;
  display: flex;
  margin-top: 24rpx;
  margin-right: 32rpx;
  margin-left: 32rpx;
  padding: 20rpx 80rpx 20rpx 32rpx;
  background-color: rgba(126, 134, 142, 0.16);
  border-radius: var(--common_border_radius_s, 4px);
}
.dtm-modal-avatar-wrap {
  margin-top: 24rpx;
  margin-right: 40rpx;
  margin-left: 40rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}
.dtm-modal-avatar-wrap .dtm-modal-avatar {
  width: 72rpx;
}
.dtm-modal-avatar-wrap .dtm-modal-avatar-nick {
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  word-break: break-all;
  font-size: var(--common_body_text_style__font-size, 17px);
  line-height: var(--common_body_text_style__line-height, 1.5);
  margin-left: 24rpx;
  color: #171a1d;
  color: var(--common_level1_base_color, #171a1d);
}
.dtm-modal-input {
  font-size: var(--common_body_text_style__font-size, 17px);
  line-height: var(--common_body_text_style__line-height, 1.5);
  color: #171a1d;
  color: var(--common_level1_base_color, #171a1d);
  flex: 1;
  word-break: initial;
  height: 48rpx;
  background: transparent;
  text-align: left;
  padding: 0;
}
.dtm-modal-input::placeholder {
  color: rgba(23, 26, 29, 0.24);
  color: var(--common_level4_base_color, rgba(23, 26, 29, 0.24));
}
.dtm-modal-delete-icon {
  position: absolute;
  right: 26rpx;
  color: rgba(23, 26, 29, 0.4);
  color: var(--common_level3_base_color, rgba(23, 26, 29, 0.4));
  line-height: 52rpx;
}
.dtm-modal-button-list {
  margin-top: 48rpx;
}
.dtm-modal-button-item {
  font-size: var(--common_body_text_style__font-size, 17px);
  line-height: var(--common_body_text_style__line-height, 1.5);
  text-align: center;
  padding: 24rpx 48rpx;
  color: #171a1d;
  color: var(--common_level1_base_color, #171a1d);
  border-top: 1rpx solid rgba(126, 134, 142, 0.16);
  border-top: 1rpx solid var(--common_line_light_color, rgba(126, 134, 142, 0.16));
}
.dtm-modal-button-item-main {
  font-size: var(--common_body_text_style__font-size, 17px);
  line-height: var(--common_body_text_style__line-height, 1.5);
  color: #007fff;
  color: var(--theme_primary1_color, #007fff);
}
.dtm-modal-button-item-waring {
  font-size: var(--common_body_text_style__font-size, 17px);
  line-height: var(--common_body_text_style__line-height, 1.5);
  color: #ff5219;
  color: var(--common_red1_color, #ff5219);
}
.dtm-modal-button-list-row {
  display: flex;
}
.dtm-modal-button-item-row {
  flex: 1;
  flex-direction: row;
}
.dtm-modal-button-item-column {
  display: flex;
  flex: 1;
  flex-direction: column;
}
.dtm-modal-button-item-line {
  padding: 24rpx 32rpx;
}
.dtm-modal-button-item-line:last-child {
  border-left: 1rpx solid rgba(126, 134, 142, 0.16);
  border-left: 1rpx solid var(--common_line_light_color, rgba(126, 134, 142, 0.16));
}
