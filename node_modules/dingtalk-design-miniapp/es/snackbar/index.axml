<view class="dtm-snackbar {{className}} {{isMove?'fadeout':''}}" a:if="{{show&&showPage}}">
  <view class="dtm-snackbar-content"  onTouchStart="touchStart" onTouchEnd="touchEnd">
    <view class="dtm-snackbar-content-box" >
      <view class="dtm-snackbar-content-left">
        <view class="dtm-snackbar-content-left-top">
          <icon a:if="{{iconType}}" className="dtm-snackbar-content-icon {{iconClassName}}" type="{{iconType}}"></icon>
          <view class="dtm-snackbar-content-title">{{content}}</view>
        </view>
      </view>
      <view class="dtm-snackbar-content-extra {{extraClassName}}" onTap="onTap">{{actionText}}</view>
    </view>
  </view>
</view>