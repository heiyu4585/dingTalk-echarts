.dingtalk-icon {
  min-width: 40rpx;
  font-family: 'dingtalkIconfont';
}
.dtm-inpage-guide {
  background-color: rgba(126, 134, 142, 0.16);
  background-color: var(--common_line_light_color, rgba(126, 134, 142, 0.16));
  border-radius: var(--common_border_radius_l, 8px);
  padding: 32rpx;
  display: flex;
  min-height: 140rpx;
  position: relative;
}
.dtm-inpage-guide.blue {
  background-color: rgba(0, 127, 255, 0.12);
  background-color: var(--theme_primary3_color, rgba(0, 127, 255, 0.12));
}
.dtm-inpage-guide-icon-wrap {
  flex: 0 0 auto;
  display: flex;
  align-items: center;
  margin-right: 32rpx;
  font-size: 72rpx;
  color: rgba(23, 26, 29, 0.6);
  color: var(--common_level2_base_color, rgba(23, 26, 29, 0.6));
}
.dtm-inpage-guide-content {
  flex: 1;
}
.dtm-inpage-guide-content-title {
  font-size: var(--common_body_bold_text_style__font-size, 17px);
  font-weight: bold;
  line-height: var(--common_body_bold_text_style__line-height, 1.5);
  color: #171a1d;
  color: var(--common_level1_base_color, #171a1d);
}
.dtm-inpage-guide-content-desc {
  font-size: var(--common_description_text_style__font-size, 14px);
  line-height: var(--common_description_text_style__line-height, 1.5);
  color: rgba(23, 26, 29, 0.6);
  color: var(--common_level2_base_color, rgba(23, 26, 29, 0.6));
  margin-top: 16rpx;
}
.dtm-inpage-guide-content-action {
  font-size: var(--common_action_text_style__font-size, 14px);
  line-height: var(--common_action_text_style__line-height, 1.5);
  color: #007fff;
  color: var(--theme_primary1_color, #007fff);
  margin-top: 16rpx;
}
.dtm-inpage-guide-img {
  margin-left: 16rpx;
  flex: 0 0 280rpx;
  align-items: center;
  display: flex;
}
.dtm-inpage-guide-img .dtm-inpage-guide-image {
  object-fit: contain;
  max-width: 280rpx;
  max-height: 200rpx;
}
.dtm-inpage-guide-close {
  position: absolute;
  right: 32rpx;
  top: 32rpx;
  font-size: 40rpx;
  color: rgba(23, 26, 29, 0.6);
  color: var(--common_level2_base_color, rgba(23, 26, 29, 0.6));
}
