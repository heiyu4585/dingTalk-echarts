.dingtalk-icon {
  min-width: 40rpx;
  font-family: 'dingtalkIconfont';
}
.dtm-nav-bar {
  height: 104rpx;
  color: var(--common_level1_base_color, #171a1d);
  display: flex;
  padding: 0 32rpx;
  border-bottom: 2rpx solid var(--common_line_light_color, rgba(126, 134, 142, 0.16));
}
.dtm-nav-bar-left,
.dtm-nav-bar-title,
.dtm-nav-bar-right {
  flex: 1;
  line-height: 104rpx;
  font-size: 32rpx;
}
.dtm-nav-bar-left {
  color: var(--common_level1_base_color, #171a1d);
}
.dtm-nav-bar-title {
  text-align: center;
}
.dtm-nav-bar-right {
  text-align: right;
  color: var(--theme_primary1_color, #007fff);
}
