<view class="dtm-nav-bar {{className}}" style="{{style}}">
  <view class="dtm-nav-bar-left">
    <icon class="dingtalk-icon {{leftClassName}}" type="{{leftIconType}}" onDmTap="onLeft" a:if="{{leftIconType}}"/>
    <view class="dtm-nav-bar-left-text" a:else onTap="onLeft">
      <view class="{{leftClassName}}">{{leftText}}</view>
    </view>
  </view>
  <view class="dtm-nav-bar-title"><slot></slot></view>
  <view class="dtm-nav-bar-right">
    <icon class="dingtalk-icon {{rightClassName}}" type="{{rightIconType}}" onDmTap="onRight" a:if="{{rightIconType}}"/>
    <view class="dtm-nav-bar-right-text" a:else onTap="onRight">
      <text class="{{rightClassName}}">{{rightText}}</text>  
    </view>
  </view>
</view>