.a-swiper-dot {
  margin: 2rpx 6rpx;
  width: 12rpx;
  height: 12rpx;
  border-radius: 50%;
  background-color: var(--common_black4_color, rgba(0, 0, 0, 0.18));
}
.a-swiper-dot.a-swiper-dot-active,
.a-swiper-dot.active {
  margin: 0rpx 6rpx;
  width: 16rpx;
  height: 16rpx;
  background-color: var(--common_black2_color, rgba(0, 0, 0, 0.4));
}
