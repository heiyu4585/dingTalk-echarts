.dtm-indicator {
  width: auto;
  margin: 16rpx 0;
}
.dtm-indicator-horizontal {
  height: 12rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.dtm-indicator-vertical {
  height: 12rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.dtm-indicator-dot {
  width: 12rpx;
  height: 12rpx;
  background: var(--common_level4_base_color, rgba(23, 26, 29, 0.24));
  border-radius: 6rpx;
  margin: 6rpx;
}
.dtm-indicator-dot-active {
  width: 16rpx;
  height: 16rpx;
  background: var(--common_action_icon_press_color, rgba(23, 26, 29, 0.4));
  border-radius: 8rpx;
  margin: 6rpx;
}
