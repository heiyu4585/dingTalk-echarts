  }
  12.5% {
    background-position: 0px -48px;
  }
  18.75% {
    background-position: 0px -72px;
  }
  25% {
    background-position: 0px -96px;
  }
  31.25% {
    background-position: 0px -120px;
  }
  37.5% {
    background-position: 0px -144px;
  }
  43.75% {
    background-position: 0px -168px;
  }
  50% {
    background-position: 0px -192px;
  }
  56.25% {
    background-position: 0px -216px;
  }
  62.5% {
    background-position: 0px -240px;
  }
  68.75% {
    background-position: 0px -264px;
  }
  75% {
    background-position: 0px -288px;
  }
  81.25% {
    background-position: 0px -312px;
  }
  87.5% {
    background-position: 0px -336px;
  }
  93.75% {
    background-position: 0px -360px;
  }
  99% {
    -webkit-transform: translateX(0);
  }
  100% {
    background-position: 0px -384px;
  }
}
.dtm-spin {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  z-index: 5000;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.dtm-spin-small .dtm-spin-lightning {
  width: 24px;
  height: 24px;
  animation: lightningAnimate-small 1s steps(1, start) infinite;
}
.dtm-spin-small .dtm-spin-text {
  font-size: var(--common_footnote_text_style__font-size, 12px);
  font-weight: normal;
  line-height: var(--common_footnote_text_style__line-height, 1.5);
}
.dtm-spin.dtm-spin-text-right {
  flex-direction: row;
}
.dtm-spin.dtm-spin-text-right .dtm-spin-text {
  margin-top: 0;
  margin-left: 4px;
}
.dtm-spin-lightning {
  width: 36px;
  height: 36px;
  background-size: 100%;
  background-repeat: no-repeat;
  background-image: url('https://img.alicdn.com/imgextra/i4/O1CN01g10n721qFTXgsj9tB_!!6000000005466-2-tps-72-1224.png');
  animation: lightningAnimate 1s steps(1, start) infinite;
}
.dtm-spin-lightning.dark {
  filter: grayscale(100%) brightness(100);
}
.dtm-spin-text {
  font-size: var(--common_action_text_style__font-size, 14px);
  line-height: var(--common_action_text_style__line-height, 1.5);
  color: rgba(23, 26, 29, 0.4);
  color: var(--common_level3_base_color, rgba(23, 26, 29, 0.4));
  margin-top: 16rpx;
}
