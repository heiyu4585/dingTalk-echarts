.dtm-popguide {
  position: relative;
}
.dtm-popguide-content {
  position: absolute;
  z-index: 4000;
  font-size: 14px;
  border-radius: var(--common_border_radius_m, 6px);
  background-color: #45484a;
  background-color: var(--popover_bg_color, #45484a);
  box-shadow: 0 4rpx 8rpx 0 rgba(126, 134, 142, 0.24);
  box-shadow: 0 4rpx 8rpx 0 var(--common_line_hard_color, rgba(126, 134, 142, 0.24));
}
.dtm-popguide-img-text-item {
  color: var(--common_white1_color, #ffffff);
  display: inline-flex;
  padding: 24rpx;
}
.dtm-popguide-img-text-item-img {
  width: 96rpx;
  height: 96rpx;
  border-radius: var(--common_border_radius_m, 6px);
  flex: 0 0 auto;
}
.dtm-popguide-img-text-item-text-content {
  display: inline-flex;
  flex-direction: column;
  justify-content: center;
  margin-left: 16rpx;
}
.dtm-popguide-img-text-item-text-content-title {
  color: var(--common_white1_color, #ffffff);
  font-size: 34rpx;
  line-height: 44rpx;
}
.dtm-popguide-img-text-item-text-content-desc {
  margin-top: 8rpx;
  color: var(--common_white2_color, rgba(255, 255, 255, 0.4));
  font-size: 28rpx;
  line-height: 44rpx;
}
.dtm-popguide-img-text-item-close {
  font-size: 36rpx;
  margin-left: 16rpx;
}
.dtm-popguide-text-item {
  color: var(--common_white1_color, #ffffff);
  font-size: 28rpx;
  display: inline-flex;
  padding: 12rpx 24rpx;
  line-height: 44rpx;
}
.dtm-popguide-text-item-close {
  font-size: 36rpx;
  margin-left: 16rpx;
  display: inline-flex;
  align-items: center;
}
.dtm-popguide-inner {
  position: relative;
  width: max-content;
  max-width: 480rpx;
  min-width: 100rpx;
  overflow: hidden;
  text-overflow: ellipsis;
}
.dtm-popguide-arrow {
  position: absolute;
  width: 7px;
  height: 7px;
  background-color: #45484a;
  background-color: var(--popover_bg_color, #45484a);
  z-index: 0;
}
.dtm-popguide-placement-top {
  top: -7px;
  left: 50%;
  transform: translate(-50%, -100%);
}
.dtm-popguide-placement-left {
  top: 50%;
  left: -7px;
  transform: translate(-100%, -50%);
}
.dtm-popguide-placement-right {
  top: 50%;
  right: -7px;
  transform: translate(100%, -50%);
}
.dtm-popguide-placement-bottom {
  bottom: -7px;
  left: 50%;
  transform: translate(-50%, 100%);
}
.dtm-popguide-placement-topLeft {
  top: -7px;
  right: 0;
  transform: translate(0, -100%);
}
.dtm-popguide-placement-topRight {
  top: -7px;
  left: 0;
  transform: translate(0, -100%);
}
.dtm-popguide-placement-bottomLeft {
  bottom: -7px;
  right: 0;
  transform: translateY(100%);
}
.dtm-popguide-placement-bottomRight {
  bottom: -7px;
  left: 0;
  transform: translateY(100%);
}
.dtm-popguide-placement-leftTop {
  top: 0;
  left: -7px;
  transform: translateX(-100%);
}
.dtm-popguide-placement-leftBottom {
  bottom: 0;
  left: -7px;
  transform: translateX(-100%);
}
.dtm-popguide-placement-rightTop {
  top: 0;
  right: -7px;
  transform: translateX(100%);
}
.dtm-popguide-placement-rightBottom {
  bottom: 0;
  right: -7px;
  transform: translateX(100%);
}
.dtm-popguide-placement-top .dtm-popguide-arrow,
.dtm-popguide-placement-topLeft .dtm-popguide-arrow,
.dtm-popguide-placement-topRight .dtm-popguide-arrow {
  transform: translateY(50%) rotate(45deg);
  bottom: 0;
}
.dtm-popguide-placement-top .dtm-popguide-arrow {
  transform: translate(-50%, 50%) rotate(45deg);
  left: 50%;
}
.dtm-popguide-placement-topRight .dtm-popguide-arrow {
  left: 16px;
}
.dtm-popguide-placement-topLeft .dtm-popguide-arrow {
  right: 16px;
}
.dtm-popguide-placement-right .dtm-popguide-arrow,
.dtm-popguide-placement-rightTop .dtm-popguide-arrow,
.dtm-popguide-placement-rightBottom .dtm-popguide-arrow {
  transform: translateX(-50%) rotate(45deg);
  left: 0;
}
.dtm-popguide-placement-right .dtm-popguide-arrow {
  transform: translate(-50%, -50%) rotate(45deg);
  top: 50%;
}
.dtm-popguide-placement-rightBottom .dtm-popguide-arrow {
  bottom: 16px;
}
.dtm-popguide-placement-rightTop .dtm-popguide-arrow {
  top: 16px;
}
.dtm-popguide-placement-bottom .dtm-popguide-arrow,
.dtm-popguide-placement-bottomLeft .dtm-popguide-arrow,
.dtm-popguide-placement-bottomRight .dtm-popguide-arrow {
  transform: translateY(-50%) rotate(45deg);
  top: 0;
}
.dtm-popguide-placement-bottom .dtm-popguide-arrow {
  transform: translate(-50%, -50%) rotate(45deg);
  left: 50%;
}
.dtm-popguide-placement-bottomLeft .dtm-popguide-arrow {
  right: 16px;
}
.dtm-popguide-placement-bottomRight .dtm-popguide-arrow {
  left: 16px;
}
.dtm-popguide-placement-left .dtm-popguide-arrow,
.dtm-popguide-placement-leftTop .dtm-popguide-arrow,
.dtm-popguide-placement-leftBottom .dtm-popguide-arrow {
  transform: translateX(50%) rotate(45deg);
  right: 0;
}
.dtm-popguide-placement-left .dtm-popguide-arrow {
  transform: translate(50%, -50%) rotate(45deg);
  top: 50%;
}
.dtm-popguide-placement-leftTop .dtm-popguide-arrow {
  top: 16px;
}
.dtm-popguide-placement-leftBottom .dtm-popguide-arrow {
  bottom: 16px;
}
