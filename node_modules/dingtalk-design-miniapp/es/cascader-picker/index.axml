<picker 
    className="dmt-cascader-picker {{className||''}}"
    popClassName="dmt-cascader-picker-popup"
    data="{{columns}}"
    value="{{currentValue}}"
    disabled="{{disabled}}"
    title="{{title}}" 
    placeholder="{{placeholder}}"
    okText="{{okText}}"
    dismissText="{{dismissText}}"
    onChange="onChange"
    onDismiss="onDismiss"
    onOk="onOk"
    onTriggerPicker="onTriggerPicker"
    maskClosable="{{maskClosable}}"
    onFormat="onFormat">
    <slot name="content"></slot>
    <slot name="title" slot="title"/>
</picker>