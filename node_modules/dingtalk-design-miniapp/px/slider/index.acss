.dtm-slider,
.dtm-slider-disabled {
  --fill-color: var(--theme_primary1_color, #007fff);
  user-select: none;
  padding: 0 20px;
  display: flex;
  align-items: center;
}
.dtm-slider-track,
.dtm-slider-disabled-track {
  position: relative;
  width: 100%;
  height: 28px;
  background-color: var(--common_fg_z1_color, #ffffff);
  display: flex;
}
.dtm-slider-track-number,
.dtm-slider-disabled-track-number {
  height: 48px;
}
.dtm-slider-track-touch-area,
.dtm-slider-disabled-track-touch-area {
  position: absolute;
  height: 100%;
  width: 100%;
}
.dtm-slider-track-fill,
.dtm-slider-disabled-track-fill {
  position: absolute;
  left: 0;
  top: 12px;
  right: 0;
  height: 3px;
}
.dtm-slider-track-fill-background,
.dtm-slider-disabled-track-fill-background {
  position: absolute;
  background-color: var(--common_line_light_color, rgba(126, 134, 142, 0.16));
  width: 100%;
  height: 100%;
  border-radius: 1px;
}
.dtm-slider-track-fill-front,
.dtm-slider-disabled-track-fill-front {
  position: absolute;
  height: 100%;
  border-radius: 1px;
  background-color: var(--fill-color);
}
.dtm-slider-end-marks,
.dtm-slider-disabled-end-marks {
  font-size: 14px;
  color: var(--common_level2_base_color, rgba(23, 26, 29, 0.6));
  margin-left: 8px;
}
.dtm-slider-handler,
.dtm-slider-disabled-handler {
  position: absolute;
  left: 0;
  width: 16px;
  height: 16px;
  box-sizing: border-box;
  transform: translate(-50%, -50%);
  top: 1px;
  background: var(--common_fg_z1_color, #ffffff);
  border-radius: 50%;
  border: 2px solid var(--fill-color);
  cursor: pointer;
}
.dtm-slider-handler:active,
.dtm-slider-disabled-handler:active {
  width: 20px;
  height: 20px;
}
.dtm-slider-handler-icon-default,
.dtm-slider-disabled-handler-icon-default {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
}
.dtm-slider-handler-icon-from-props,
.dtm-slider-disabled-handler-icon-from-props {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  text-align: center;
  line-height: 100%;
}
.dtm-slider-handler-icon-from-props-icon,
.dtm-slider-disabled-handler-icon-from-props-icon {
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
}
.dtm-slider-tick,
.dtm-slider-disabled-tick {
  position: absolute;
  width: 8px;
  height: 8px;
  box-sizing: border-box;
  border-radius: 50%;
  top: 2px;
  transform: translate(-50%, -50%);
}
.dtm-slider-tick-front,
.dtm-slider-disabled-tick-front {
  background-color: var(--fill-color);
}
.dtm-slider-tick-back,
.dtm-slider-disabled-tick-back {
  background-color: var(--common_fg_z1_color, #ffffff);
  border: 2px solid var(--common_line_light_color, rgba(126, 134, 142, 0.16));
}
.dtm-slider-tick-number,
.dtm-slider-disabled-tick-number {
  position: absolute;
  color: var(--common_level2_base_color, rgba(23, 26, 29, 0.6));
  font-size: 14px;
  transform: translateX(-50%);
  top: 10px;
  left: 4px;
  text-align: center;
  line-height: 22px;
  height: 22px;
}
.dtm-slider-disabled {
  cursor: not-allowed;
}
.dtm-slider-disabled .dtm-slider-tick-number {
  color: var(--common_level4_base_color, rgba(23, 26, 29, 0.24));
}
.dtm-slider-disabled .dtm-slider-tick-front {
  background-color: var(--common_fg_z1_color, #ffffff);
}
.dtm-slider-disabled .dtm-slider-tick-front::after {
  position: absolute;
  width: 100%;
  height: 100%;
  background-color: var(--theme_primary2_color, rgba(0, 127, 255, 0.48));
  border-radius: 50%;
  content: '';
}
.dtm-slider-disabled .dtm-slider-track-fill-front {
  background-color: var(--theme_primary2_color, rgba(0, 127, 255, 0.48));
}
.dtm-slider-disabled .dtm-slider-handler {
  border: 2px solid var(--theme_primary2_color, rgba(0, 127, 255, 0.48));
}
