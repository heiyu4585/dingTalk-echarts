.dtm-list-item {
  position: relative;
  white-space: pre-line;
  word-break: break-all;
  background-color: #ffffff;
  background-color: var(--common_fg_z1_color, #ffffff);
}
.dtm-list-item:last-child .dtm-list-item-border {
  background-color: transparent;
}
.dtm-list-item:last-child .hasborder {
  background-color: rgba(126, 134, 142, 0.16);
  background-color: var(--common_line_light_color, rgba(126, 134, 142, 0.16));
}
.dtm-list-item-content {
  position: relative;
  display: flex;
  align-items: center;
  box-sizing: border-box;
  padding-left: 16px;
  flex-direction: row;
}
.dtm-list-item .dtm-list-item-main {
  position: relative;
  display: flex;
  flex: 1;
  flex-direction: row;
  align-items: center;
  box-sizing: border-box;
  min-height: 56px;
  padding-right: 16px;
  overflow: hidden;
}
.dtm-list-item .dtm-list-item-main.border {
  border-bottom: 1px solid rgba(126, 134, 142, 0.16);
  border-bottom: 1px solid var(--common_line_light_color, rgba(126, 134, 142, 0.16));
}
.dtm-list-item .dtm-list-item-main.large {
  position: relative;
  display: flex;
  flex: 1;
  flex-direction: row;
  align-items: center;
  box-sizing: border-box;
  min-height: 56px;
  padding-right: 16px;
  overflow: hidden;
}
.dtm-list-item .dtm-list-item-main-content {
  display: flex;
  flex: 1;
  flex-direction: column;
  height: 100%;
  padding-top: 16px;
  padding-bottom: 16px;
}
.dtm-list-item .dtm-list-item-main-content.multi-text {
  padding-top: 12px;
  padding-bottom: 12px;
}
.dtm-list-item-thumb-wrapper {
  margin-right: 13px;
}
.dtm-list-item-thumb-wrapper.thumbVTop {
  align-self: flex-start;
  margin-top: 16px;
}
.dtm-list-item-thumb-wrapper .dtm-list-item-thumb {
  display: flex;
  flex-direction: row;
  background-repeat: no-repeat;
  background-position: center;
  background-size: 100% 100%;
  background-size: cover;
}
.dtm-list-item-thumb-wrapper .dtm-list-item-thumb.mini,
.dtm-list-item-thumb-wrapper .dtm-list-item-thumb.small {
  width: 21px;
  height: 21px;
}
.dtm-list-item-thumb-wrapper .dtm-list-item-thumb.standard {
  width: 36px;
  height: 36px;
}
.dtm-list-item-thumb-wrapper .dtm-list-item-thumb.big {
  width: 48px;
  height: 48px;
}
.dtm-list-item-thumb-wrapper .dtm-list-item-thumb.large {
  width: 56px;
  height: 56px;
}
.dtm-list-item-thumb-wrapper .dtm-list-item-thumb.square {
  overflow: hidden;
  border-radius: var(--common_border_radius_s, 4px);
}
.dtm-list-item-thumb-wrapper .dtm-list-item-thumb.circle {
  overflow: hidden;
  border-radius: 50%;
}
.dtm-list-item-thumb-wrapper .dtm-list-item-thumb.default {
  background-color: rgba(0, 127, 255, 0.48);
  background-color: var(--common_blue2_color, rgba(0, 127, 255, 0.48));
}
.dtm-list-item .dtm-list-item-thumb-content {
  color: #ffffff;
  color: var(--common_white1_color, #ffffff);
  background-color: #007fff;
  background-color: var(--common_blue1_color, #007fff);
  justify-content: center;
}
.dtm-list-item .dtm-list-item-caption {
  font-size: 17px;
  line-height: 1.2;
  color: #171a1d;
  color: var(--common_level1_base_color, #171a1d);
}
.dtm-list-item .dtm-list-item-caption.disabled {
  color: rgba(23, 26, 29, 0.24);
  color: var(--common_level4_base_color, rgba(23, 26, 29, 0.24));
}
.dtm-list-item .dtm-list-item-border {
  position: absolute;
  right: 0;
  bottom: 1px;
  left: 16px;
  height: 1px;
  background-color: rgba(126, 134, 142, 0.16);
  background-color: var(--common_line_light_color, rgba(126, 134, 142, 0.16));
  transform: scaleY(0.5);
}
.dtm-list-item .dtm-list-item-border.b-s {
  left: 56px;
}
.dtm-list-item .dtm-list-item-brief-item {
  font-size: var(--common_description_text_style__font-size, 14px);
  line-height: var(--common_description_text_style__line-height, 1.5);
  color: rgba(23, 26, 29, 0.4);
  color: var(--common_level3_base_color, rgba(23, 26, 29, 0.4));
}
.dtm-list-item .dtm-list-item-brief-no-margin {
  margin-top: 0;
}
.dtm-list-item .dtm-list-item-extra {
  display: flex;
  align-items: center;
  overflow: hidden;
  font-size: var(--common_h2_text_style__font-size, 18px);
  line-height: var(--common_h2_text_style__line-height, 1.4);
}
.dtm-list-item .dtm-list-item-extra-text {
  font-size: var(--common_action_text_style__font-size, 14px);
  line-height: var(--common_action_text_style__line-height, 1.5);
  color: rgba(23, 26, 29, 0.4);
  color: var(--common_level3_base_color, rgba(23, 26, 29, 0.4));
  text-overflow: ellipsis;
  white-space: nowrap;
  max-width: 120px;
  padding-left: 16px;
  overflow: hidden;
}
.dtm-list-item .dtm-list-item-extra-text.disabled {
  color: rgba(23, 26, 29, 0.24);
  color: var(--common_level4_base_color, rgba(23, 26, 29, 0.24));
}
.dtm-list-item .dtm-list-item-extra-valign-top {
  align-self: flex-start;
  margin-top: 16px;
}
.dtm-list-item .dtm-list-item-extra .dtm-icon {
  margin-right: 0;
}
.dtm-list-item .dtm-list-item-badge {
  display: flex;
  align-items: center;
  margin-left: 5px;
}
.dtm-list-item .dtm-list-item-wrapper-arrow {
  display: flex;
  align-items: center;
  text-align: right;
  color: rgba(23, 26, 29, 0.4);
  color: var(--common_level3_base_color, rgba(23, 26, 29, 0.4));
}
.dtm-list-item .dtm-list-item-wrapper-arrow.width-badge {
  padding-left: 10px;
}
.dtm-list-item .dtm-list-item-checkbox-wrapper {
  width: 24px;
  height: 24px;
  margin-right: 12px;
  line-height: 24px;
  text-align: center;
}
.dtm-list-item:active {
  background-color: rgba(126, 134, 142, 0.24);
  background-color: var(--common_overlay_press_color, rgba(126, 134, 142, 0.24));
}
.dtm-list-item.hover-none {
  background-color: #ffffff;
  background-color: var(--common_fg_z1_color, #ffffff);
}
.dtm-list-item .dtm-list-item-error {
  font-size: var(--common_description_text_style__font-size, 14px);
  line-height: var(--common_description_text_style__line-height, 1.5);
  box-sizing: border-box;
  color: #ff5219;
  color: var(--common_red1_color, #ff5219);
  line-height: 16px;
  border-top: 1px solid #ff5219;
  border-top: 1px solid var(--common_red1_color, #ff5219);
  margin: 10px 16px 0 16px;
  padding: 8px 0;
}
.dtm-list-item .dtm-list-item-required {
  position: absolute;
  top: 20px;
  left: 6px;
  display: inline-block;
  color: #ff5219;
  color: var(--common_red1_color, #ff5219);
  font-size: 8px;
  line-height: 18px;
}
