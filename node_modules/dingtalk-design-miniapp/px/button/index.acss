@keyframes loadingCircle {
  100% {
    transform: rotate(360deg);
  }
}
@font-face {
  font-family: 'dingtalkIconfont';
  src: url('//at.alicdn.com/t/font_135284_1tubh9156fi.ttf');
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.dingtalk-icon {
  font-family: 'dingtalkIconfont';
}
.dtm-button {
  position: relative;
  display: inline-block;
  overflow: visible;
  border-radius: var(--common_border_radius_l, 8px);
  box-sizing: border-box;
  transition: all 0.3s cubic-bezier(0.645, 0.045, 0.355, 1);
}
.dtm-button-button {
  box-sizing: border-box;
  margin-top: 0;
  margin-right: 0;
  margin-bottom: 0;
  margin-left: 0;
  border-radius: var(--common_border_radius_l, 8px);
  padding-right: 16px;
  padding-left: 16px;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  border: none;
}
.dtm-button-button .dtm-button-text {
  display: inline;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.dtm-button::before {
  position: absolute;
  top: -1px;
  right: -1px;
  bottom: -1px;
  left: -1px;
  z-index: 1;
  display: block;
  border-radius: inherit;
  opacity: 0.35;
  transition: opacity 0.2s;
  content: '';
  pointer-events: none;
}
.dtm-button .loading-wrap {
  display: flex;
  justify-content: center;
  align-items: center;
  height: 16px;
  width: 16px;
}
.dtm-button-loading-icon {
  display: flex;
  justify-content: center;
  align-items: center;
  height: 16px;
  width: 16px;
  font-size: 16px;
  color: #ffffff;
  color: var(--common_white1_color, #ffffff);
  animation: loadingCircle 1s infinite linear;
}
.dtm-button-primary .dtm-button-button,
.dtm-button-primary.dtm-button-loading .dtm-button-button,
.dtm-button-primary .dtm-button-button:active,
.dtm-button-primary.dtm-button-loading .dtm-button-button:active {
  color: #ffffff;
  color: var(--common_white1_color, #ffffff);
  background-color: #007fff;
  background-color: var(--theme_primary1_color, #007fff);
}
.dtm-button-primary .dtm-button-button:active {
  color: #ffffff;
  color: var(--common_white1_color, #ffffff);
  background: #006ad6;
  background: var(--theme_primary_press_color, #006ad6);
}
.dtm-button-primary.dtm-button-disable .dtm-button-button,
.dtm-button-primary.dtm-button-disable .dtm-button-button:active {
  color: rgba(255, 255, 255, 0.4);
  color: var(--common_white2_color, rgba(255, 255, 255, 0.4));
  background: rgba(0, 127, 255, 0.48);
  background: var(--theme_primary2_color, rgba(0, 127, 255, 0.48));
}
.dtm-button-default .dtm-button-button,
.dtm-button-default.dtm-button-loading .dtm-button-button,
.dtm-button-default .dtm-button-button:active,
.dtm-button-default.dtm-button-loading .dtm-button-button:active {
  color: #171a1d;
  color: var(--common_level1_base_color, #171a1d);
  background-color: #FFFFFF;
  background-color: var(--button_secondary_bg_color, #FFFFFF);
  border: 1px solid var(--button_secondary_line_color, rgba(126, 134, 142, 0.24));
}
.dtm-button-default .dtm-button-button:active {
  color: #171a1d;
  color: var(--common_level1_base_color, #171a1d);
}
.dtm-button-default .dtm-button-button:active::after {
  content: '';
  position: absolute;
  width: 100%;
  height: 100%;
  left: 0;
  top: 0;
  border-radius: inherit;
  background-color: var(--common_overlay_press_color, rgba(126, 134, 142, 0.24));
}
.dtm-button-default .dtm-button-loading-icon {
  color: #171a1d;
  color: var(--common_level1_base_color, #171a1d);
}
.dtm-button-default.dtm-button-disable .dtm-button-button,
.dtm-button-default.dtm-button-disable .dtm-button-button:active {
  background-color: #FFFFFF;
  background-color: var(--button_secondary_bg_color, #FFFFFF);
  color: rgba(23, 26, 29, 0.24);
  color: var(--common_level4_base_color, rgba(23, 26, 29, 0.24));
}
.dtm-button-default-light .dtm-button-button {
  color: #171a1d;
  color: var(--common_level1_base_color, #171a1d);
  background-color: #ffffff;
  background-color: var(--common_bg_z1_color, #ffffff);
}
.dtm-button-default-light .dtm-button-loading-icon {
  color: rgba(23, 26, 29, 0.6);
  color: var(--common_level2_base_color, rgba(23, 26, 29, 0.6));
}
.dtm-button-primary-ghost .dtm-button-button {
  color: #007fff;
  color: var(--theme_primary1_color, #007fff);
  background-color: transparent;
  border: 1px solid #007fff;
  border: 1px solid var(--theme_primary1_color, #007fff);
}
.dtm-button-primary-ghost .dtm-button-loading-icon {
  color: #007fff;
  color: var(--theme_primary1_color, #007fff);
}
.dtm-button-primary-ghost:active.dtm-button-button {
  color: #ffffff;
  color: var(--common_white1_color, #ffffff);
  background-color: #007fff;
  background-color: var(--theme_primary1_color, #007fff);
}
.dtm-button-primary-ghost:active.dtm-button-loading .dtm-button-button {
  color: #007fff;
  color: var(--theme_primary1_color, #007fff);
  background-color: transparent;
}
.dtm-button-danger .dtm-button-button,
.dtm-button-danger.dtm-button-loading .dtm-button-button,
.dtm-button-danger .dtm-button-button:active,
.dtm-button-danger.dtm-button-loading .dtm-button-button:active {
  color: #ffffff;
  color: var(--common_white1_color, #ffffff);
  background-color: #ff5219;
  background-color: var(--theme_danger1_color, #ff5219);
}
.dtm-button-danger .dtm-button-button:active {
  color: #ffffff;
  color: var(--common_white1_color, #ffffff);
  background: #d64414;
  background: var(--theme_danger_press_color, #d64414);
}
.dtm-button-danger.dtm-button-disable .dtm-button-button,
.dtm-button-danger.dtm-button-disable .dtm-button-button:active {
  color: rgba(255, 255, 255, 0.4);
  color: var(--common_white2_color, rgba(255, 255, 255, 0.4));
  background-color: rgba(255, 82, 25, 0.48);
  background-color: var(--theme_danger2_color, rgba(255, 82, 25, 0.48));
}
.dtm-button-size-large .dtm-button-button {
  font-size: var(--common_body_text_style__font-size, 17px);
  line-height: var(--common_body_text_style__line-height, 1.5);
  text-align: center;
  width: 100%;
}
.dtm-button-size-large .loading-wrap {
  margin-right: 6px;
}
.dtm-button-size-default,
.dtm-button-size-middle {
  height: 34px;
}
.dtm-button-size-default .dtm-button-button,
.dtm-button-size-middle .dtm-button-button {
  border-radius: var(--common_border_radius_m, 6px);
  font-size: var(--common_action_text_style__font-size, 14px);
  line-height: var(--common_action_text_style__line-height, 1.5);
  height: 34px;
  line-height: 32px;
  width: 100%;
}
.dtm-button-size-default .loading-wrap,
.dtm-button-size-middle .loading-wrap {
  position: absolute;
  right: 0;
  left: 0;
  width: 100%;
}
.dtm-button-size-default.dtm-button-loading .dtm-button-button,
.dtm-button-size-middle.dtm-button-loading .dtm-button-button,
.dtm-button-size-default:active.dtm-button-loading .dtm-button-button,
.dtm-button-size-middle:active.dtm-button-loading .dtm-button-button {
  color: transparent;
}
.dtm-button-size-small {
  height: 28px;
  border-radius: var(--common_border_radius_s, 4px);
}
.dtm-button-size-small .dtm-button-button {
  font-size: var(--common_action_text_style__font-size, 14px);
  line-height: var(--common_action_text_style__line-height, 1.5);
  height: 28px;
  line-height: 26px;
  border-radius: var(--common_border_radius_s, 4px);
  width: 100%;
  padding-right: 8px;
  padding-left: 8px;
}
.dtm-button-size-small .dtm-button-loading-icon {
  position: absolute;
  right: 0;
  left: 0;
  width: 100%;
}
.dtm-button-size-small.dtm-button-loading .dtm-button-button,
.dtm-button-size-small:active.dtm-button-loading .dtm-button-button {
  color: transparent;
}
