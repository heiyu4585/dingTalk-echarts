<list-item
  className="dm-checkbox-item-list disableChecked {{ disabled && checkable?'disableChecked':''}} {{className}}"
  disabled="{{disabled}}"
  arrowType="{{hasArrowIcon?arrowType:''}}"
  arrowSize="{{hasArrowIcon?arrowSize: ''}}"
  index="{{index}}"
  checkable="{{true}}"
  indeterminate="{{indeterminate}}"
  checked="{{isChecked !== null ? isChecked : checked}}"
  onArrowTap="onItemExtraTap"
  onChange="onItemChange"
>
  {{itemName}}
</list-item>