.iconfont {
  color: var(--common_overlay_normal_color, rgba(255, 255, 255, 0.4));
  margin-left: 6px;
  font-size: 18px;
}
.dtm-snackbar {
  width: 100vw;
  position: fixed;
  bottom: 34px;
  left: 0;
  padding: 0 16px;
  box-sizing: border-box;
  font-size: 14px;
}
.dtm-snackbar.fadeout {
  -webkit-transition: all 0.5s;
  -moz-transition: all 0.5s;
  -ms-transition: all 0.5s;
  -o-transition: all 0.5s;
  transition: all 0.5s;
  opacity: 0;
}
.dtm-snackbar-content {
  background: var(--common_tips_bg_color, rgba(69, 72, 74, 0.9));
  box-shadow: 0 8px 24px 0 var(--common_overlay_color, rgba(0, 0, 0, 0.16));
  border-radius: 6px;
  background-color: var(--popover_bg_color, #45484a);
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.dtm-snackbar-content-box {
  padding: 12px 16px;
  line-height: 22px;
  flex: 1;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.dtm-snackbar-content-left {
  flex: 1;
}
.dtm-snackbar-content-left-top {
  display: flex;
  align-items: center;
}
.dtm-snackbar-content-icon {
  margin-right: 8px;
  font-size: 17px;
  color: var(--common_white1_color, #ffffff);
}
.dtm-snackbar-content-title {
  color: var(--common_white1_color, #ffffff);
}
.dtm-snackbar-content-sub-title {
  margin-top: 4px;
  font-size: 12px;
  color: var(--common_overlay_normal_color, rgba(255, 255, 255, 0.4));
}
.dtm-snackbar-content-extra {
  color: var(--common_water1_color, #5ac8fa);
  text-align: right;
  max-width: 90px;
  margin-left: 12px;
}
.dtm-snackbar-closes {
  padding-right: 12px;
}
.dtm-snackbar-close {
  padding: 12px 12px 12px 0;
}
