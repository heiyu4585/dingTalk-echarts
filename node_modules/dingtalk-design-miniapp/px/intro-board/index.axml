<view class="dtm-intro-board {{className}} {{isMove?'fadeout':''}}" a:if="{{show&&showPage}}">
  <view class="dtm-intro-board-content"  onTouchStart="touchStart" onTouchEnd="touchEnd">
    <view class="dtm-intro-board-content-box" onTap="onTap">
      <view class="dtm-intro-board-content-left">
        <view class="dtm-intro-board-content-left-top">
          <image class="dtm-intro-board-content-icon {{iconClassName}}" src="{{iconUrl}}" a:if="{{iconUrl}}" mode="aspectFit"/>
          <view class="dtm-intro-board-content-title">{{title}}</view>
        </view>
        <view class="dtm-intro-board-content-sub-title" a:if="{{subTitle}}">{{subTitle}}</view> 
      </view>
      <view class="dtm-intro-board-content-extra {{extraClassName}}">{{extraText}}</view>
    </view>
    <view class="dtm-intro-board-closes {{subTitle?'dtm-intro-board-close':''}}" onTap="onClose">
      <icon className="iconfont {{arrowClassName}}" type="close"></icon>
    </view>
  </view>
</view>