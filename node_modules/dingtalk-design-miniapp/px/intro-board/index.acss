.iconfont {
  color: var(--common_overlay_normal_color, rgba(255, 255, 255, 0.4));
  margin-left: 6px;
  font-size: 18px;
}
.dtm-intro-board {
  width: 100vw;
  position: fixed;
  bottom: 34px;
  left: 0;
  padding: 0 16px;
  box-sizing: border-box;
  font-size: 14px;
}
.dtm-intro-board.fadeout {
  -webkit-transition: all 0.5s;
  -moz-transition: all 0.5s;
  -ms-transition: all 0.5s;
  -o-transition: all 0.5s;
  transition: all 0.5s;
  opacity: 0;
}
.dtm-intro-board-content {
  background: var(--common_tips_bg_color, rgba(69, 72, 74, 0.9));
  box-shadow: 0 8px 24px 0 var(--common_overlay_color, rgba(0, 0, 0, 0.16));
  border-radius: 6px;
  background-color: var(--popover_bg_color, #45484a);
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.dtm-intro-board-content-box {
  padding: 12px 0 12px 12px;
  flex: 1;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.dtm-intro-board-content-left {
  flex: 1;
}
.dtm-intro-board-content-left-top {
  display: flex;
  align-items: center;
}
.dtm-intro-board-content-icon {
  width: 18px;
  height: 18px;
  margin-right: 3px;
}
.dtm-intro-board-content-title {
  color: var(--common_bg_color, #f2f2f6);
}
.dtm-intro-board-content-sub-title {
  margin-top: 4px;
  font-size: 12px;
  color: var(--common_overlay_normal_color, rgba(255, 255, 255, 0.4));
}
.dtm-intro-board-content-extra {
  color: var(--common_water1_color, #5ac8fa);
  text-align: right;
  max-width: 90px;
  margin-left: 12px;
}
.dtm-intro-board-closes {
  padding-right: 12px;
}
.dtm-intro-board-close {
  padding: 12px 12px 12px 0;
}
