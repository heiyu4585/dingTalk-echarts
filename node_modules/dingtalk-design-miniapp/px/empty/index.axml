<view class="dtm-empty {{ inline || inpage ? 'dtm-empty-inpage' : 'dtm-empty-fullpage'}} {{className}}" style="{{style}}">
  <view class="dtm-empty-content">
    <image class="dtm-empty-img" src="{{imgObj[theme]}}" mode="widthFix" a:if="{{imgObj[theme]}}"/>
    <slot name="topIconSlot" a:if="{{iconType}}">
      <icon className="dtm-empty-icon" type="{{iconType}}"></icon>
    </slot>
    <block a:if="{{title}}">
      <view class="dtm-empty-title">{{title}}</view>
    </block>
    <block a:if="{{describe && describe.indexOf('<br/>') == -1}}">
      <view class="dtm-empty-describe" >
        {{describe}}
      </view>
    </block>
    <view class="dtm-empty-describe" a:if="{{describeArr.length}}">
      <view class="dtm-empty-describe-wrap" a:for="{{describeArr}}">{{item}}</view>
    </view>
  
    <view class="dtm-empty-link" a:if="{{linkText}}" onTap="onDmEmptyLinkTap">{{linkText}}</view>
    
    <button className="dtm-empty-btn" type="{{btnType}}" onTap="onTap" a:if="{{btnText || btnName}}">{{btnText || btnName}}</button>
  </view>
</view>