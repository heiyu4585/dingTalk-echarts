.dtm-empty {
  display: flex;
  align-items: center;
  justify-content: center;
  text-align: center;
  z-index: 4900;
  min-height: 332px;
}
.dtm-empty-inpage {
  background: transparent;
}
.dtm-empty-inpage .dtm-empty-content {
  margin-top: 0;
}
.dtm-empty-fullpage {
  background: #f2f2f6;
  background: var(--common_bg_color, #f2f2f6);
  position: fixed;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
}
.dtm-empty-content {
  padding: 60px 40px;
  box-sizing: border-box;
  margin-top: -80px;
  word-break: break-word;
}
.dtm-empty-icon {
  display: inline-block;
  width: 72px;
  color: rgba(23, 26, 29, 0.4);
  color: var(--common_level3_base_color, rgba(23, 26, 29, 0.4));
  font-size: 72px;
  text-align: center;
}
.dtm-empty-img {
  width: 180px;
  height: 180px;
}
.dtm-empty-title {
  font-size: var(--common_h2_text_style__font-size, 18px);
  line-height: var(--common_h2_text_style__line-height, 1.4);
  margin-top: 5px;
  color: #171a1d;
  color: var(--common_level1_base_color, #171a1d);
}
.dtm-empty-describe {
  font-size: var(--common_action_text_style__font-size, 14px);
  line-height: var(--common_action_text_style__line-height, 1.5);
  margin-top: 8px;
  color: rgba(23, 26, 29, 0.4);
  color: var(--common_level3_base_color, rgba(23, 26, 29, 0.4));
}
.dtm-empty-link {
  font-size: var(--common_action_text_style__font-size, 14px);
  line-height: var(--common_action_text_style__line-height, 1.5);
  color: #007fff;
  color: var(--common_blue1_color, #007fff);
}
.dtm-empty-btn {
  min-width: 102px;
  max-width: calc(100vw - 80px);
  margin-top: 14px;
}
.dtm-empty-describe-wrap {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
