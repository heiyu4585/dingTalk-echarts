@font-face {
  font-family: 'dingtalkIconfont';
  src: url('//at.alicdn.com/t/font_2278296_or56j0u661.eot');
  src: url('//at.alicdn.com/t/font_2278296_or56j0u661.eot?#iefix') format('embedded-opentype'), url('//at.alicdn.com/t/font_2278296_or56j0u661.woff2') format('woff2'), url('//at.alicdn.com/t/font_2278296_or56j0u661.woff') format('woff'), url('//at.alicdn.com/t/font_2278296_or56j0u661.ttf') format('truetype'), url('//at.alicdn.com/t/font_2278296_or56j0u661.svg#iconfont') format('svg');
}
.dingtalk-icon {
  min-width: 20px;
  font-family: 'dingtalkIconfont';
}
.dtm-notice-bar {
  font-size: var(--common_action_text_style__font-size, 14px);
  line-height: var(--common_action_text_style__line-height, 1.5);
  position: relative;
  display: flex;
  flex-direction: row;
  align-items: center;
  min-height: 44px;
  max-height: 64px;
  border-radius: var(--common_border_radius_l, 8px);
}
.dtm-notice-bar-icon {
  padding-right: 8px;
  padding-left: 12px;
  font-size: 20px;
}
.dtm-notice-bar-content {
  padding-right: 15px;
  display: -webkit-box;
  box-sizing: border-box;
  width: 313px;
  margin-top: 12px;
  margin-bottom: 12px;
  overflow: hidden;
  line-height: 20px;
  white-space: normal;
  text-overflow: ellipsis;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}
.dtm-notice-bar-content-text {
  text-overflow: ellipsis;
  lines: 2;
}
.dtm-notice-bar-info {
  color: #007fff;
  color: var(--common_blue1_color, #007fff);
  background-color: rgba(0, 127, 255, 0.12);
  background-color: var(--common_blue3_color, rgba(0, 127, 255, 0.12));
}
.dtm-notice-bar-icon-info {
  color: rgba(23, 26, 29, 0.4);
  color: var(--common_level3_base_color, rgba(23, 26, 29, 0.4));
}
.dtm-notice-bar-warn {
  color: #ff9200;
  color: var(--common_orange1_color, #ff9200);
  background-color: rgba(255, 146, 0, 0.12);
  background-color: var(--common_orange3_color, rgba(255, 146, 0, 0.12));
}
.dtm-notice-bar-error {
  color: #ff5219;
  color: var(--common_red1_color, #ff5219);
  background-color: rgba(255, 82, 25, 0.12);
  background-color: var(--common_red3_color, rgba(255, 82, 25, 0.12));
}
.dtm-notice-bar-close {
  margin-right: 12px;
  margin-left: 8px;
  font-size: 15px;
  color: rgba(23, 26, 29, 0.4);
  color: var(--common_level3_base_color, rgba(23, 26, 29, 0.4));
}
.dtm-notice-bar-extra {
  flex: 0 0 auto;
  margin-right: 12px;
  margin-left: 8px;
  color: rgba(23, 26, 29, 0.4);
  color: var(--common_level3_base_color, rgba(23, 26, 29, 0.4));
}
.dtm-notice-bar-extra .dtm-icon {
  font-size: 15px;
}
.dtm-notice-bar-closeable-content {
  width: 287px;
  padding-right: 0;
}
