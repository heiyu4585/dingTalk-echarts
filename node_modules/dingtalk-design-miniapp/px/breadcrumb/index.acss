.dtm-crumbs::-webkit-scrollbar {
  display: none;
}
.dtm-crumbs {
  display: flex;
  flex-direction: row;
  width: 100%;
  height: 48px;
  line-height: 48px;
  white-space: nowrap;
  -webkit-overflow-scrolling: touch;
  background-color: #ffffff;
  background-color: var(--common_bg_z1_color, #ffffff);
}
.dtm-crumbs-item {
  font-size: var(--common_action_text_style__font-size, 14px);
  line-height: var(--common_action_text_style__line-height, 1.5);
  display: inline-block;
  display: flex;
  align-items: center;
  justify-content: center;
  max-width: 272px;
  margin-right: 12px;
  margin-left: 12px;
  color: rgba(23, 26, 29, 0.6);
  color: var(--common_level2_base_color, rgba(23, 26, 29, 0.6));
  white-space: nowrap;
  text-overflow: ellipsis;
  vertical-align: middle;
  flex-shrink: 0;
}
.dtm-crumbs-item-first-child {
  padding-left: 16px;
}
.dtm-crumbs-item-last-child {
  padding-right: 16px;
}
.dtm-crumbs-silent {
  color: #171a1d;
  color: var(--common_level1_base_color, #171a1d);
}
.dtm-crumbs-arrow {
  display: flex;
  align-items: center;
  justify-content: center;
  color: rgba(23, 26, 29, 0.4);
  color: var(--common_level3_base_color, rgba(23, 26, 29, 0.4));
}
