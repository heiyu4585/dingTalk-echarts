const classnames = (...rest) => {
  const classNameList = rest.reduce((acc, item) => {
    if (typeof item === 'string') {
      acc.push(item);
    } else if (typeof item === 'object') {
      const subClassNameList = Object.keys(item)
        .filter(key => item[key]);
      acc = acc.concat(subClassNameList);
    }
    return acc;
  }, []);
  const classNameString = classNameList
    .filter(str => str)
    .join(' ');
  return classNameString;
}

export default classnames;