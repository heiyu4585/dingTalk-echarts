.dtm-tag {
  display: inline-block;
  flex-direction: row;
  align-items: center;
  box-sizing: border-box;
  max-width: 375px;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  flex-shrink: 0;
  border-radius: var(--common_border_radius_s, 4px);
  line-height: 1;
}
.dtm-tag-content {
  height: 100%;
}
.dtm-tag .d-f {
  display: flex;
  flex: row;
}
.dtm-tag-text {
  display: flex;
  justify-content: center;
  align-items: center;
  line-height: normal;
  height: 100%;
}
.dtm-tag-small {
  font-size: 10px;
  line-height: 16px;
  padding-right: 4px;
  padding-left: 4px;
  vertical-align: middle;
}
.dtm-tag-small .dtm-tag-close {
  min-width: 14px;
  margin-left: 2px;
}
.dtm-tag-small .dtm-tag-icon {
  min-width: 14px;
  margin-right: 2px;
}
.dtm-tag-small.dtm-tag-blue {
  border: 1px solid rgba(0, 127, 255, 0.48);
  border: 1px solid var(--unfilled-border-color, var(--common_blue2_color, rgba(0, 127, 255, 0.48)));
}
.dtm-tag-small.dtm-tag-red {
  border: 1px solid rgba(255, 82, 25, 0.48);
  border: 1px solid var(--unfilled-border-color, var(--common_red2_color, rgba(255, 82, 25, 0.48)));
}
.dtm-tag-small.dtm-tag-green {
  border: 1px solid rgba(0, 176, 66, 0.48);
  border: 1px solid var(--unfilled-border-color, var(--common_green2_color, rgba(0, 176, 66, 0.48)));
}
.dtm-tag-small.dtm-tag-orange {
  border: 1px solid rgba(255, 146, 0, 0.48);
  border: 1px solid var(--unfilled-border-color, var(--common_orange2_color, rgba(255, 146, 0, 0.48)));
}
.dtm-tag-small.dtm-tag-dark,
.dtm-tag-small.dtm-tag-default {
  color: rgba(23, 26, 29, 0.6);
  color: var(--unfilled-text-color, var(--common_level2_base_color, rgba(23, 26, 29, 0.6)));
  border: 1px solid rgba(23, 26, 29, 0.24);
  border: 1px solid var(--unfilled-border-color, var(--common_level4_base_color, rgba(23, 26, 29, 0.24)));
}
.dtm-tag-small.dtm-tag-water {
  border: 1px solid rgba(0, 127, 255, 0.48);
  border: 1px solid var(--unfilled-border-color, var(--common_blue2_color, rgba(0, 127, 255, 0.48)));
}
.dtm-tag-large {
  font-size: var(--common_action_text_style__font-size, 14px);
  line-height: var(--common_action_text_style__line-height, 1.5);
  padding-right: 10px;
  padding-left: 10px;
  height: 28px;
  line-height: 28px;
}
.dtm-tag-large .dtm-tag-close {
  min-width: 14px;
  margin-left: 5px;
}
.dtm-tag-large .dtm-tag-icon {
  min-width: 14px;
  margin-right: 5px;
}
.dtm-tag-large.dtm-tag-blue {
  background-color: rgba(0, 127, 255, 0.12);
  background-color: var(--common_blue3_color, rgba(0, 127, 255, 0.12));
}
.dtm-tag-large.dtm-tag-red {
  background-color: rgba(255, 82, 25, 0.12);
  background-color: var(--common_red3_color, rgba(255, 82, 25, 0.12));
}
.dtm-tag-large.dtm-tag-green {
  background-color: rgba(0, 176, 66, 0.12);
  background-color: var(--common_green3_color, rgba(0, 176, 66, 0.12));
}
.dtm-tag-large.dtm-tag-orange {
  background-color: rgba(255, 146, 0, 0.12);
  background-color: var(--common_orange3_color, rgba(255, 146, 0, 0.12));
}
.dtm-tag-large.dtm-tag-dark,
.dtm-tag-large.dtm-tag-default {
  color: #171a1d;
  color: var(--common_level1_base_color, #171a1d);
  background-color: rgba(126, 134, 142, 0.16);
  background-color: var(--common_line_light_color, rgba(126, 134, 142, 0.16));
}
.dtm-tag-large.dtm-tag-water {
  background-color: rgba(90, 200, 250, 0.12);
  background-color: var(--common_water3_color, rgba(90, 200, 250, 0.12));
}
.dtm-tag-close {
  display: inline-block;
  color: rgba(23, 26, 29, 0.4);
  color: var(--common_level3_base_color, rgba(23, 26, 29, 0.4));
}
.dtm-tag-blue {
  color: #007fff;
  color: var(--unfilled-text-color, var(--common_blue1_color, #007fff));
}
.dtm-tag-red {
  color: #ff5219;
  color: var(--unfilled-text-color, var(--common_red1_color, #ff5219));
}
.dtm-tag-green {
  color: #00b042;
  color: var(--unfilled-text-color, var(--common_green1_color, #00b042));
}
.dtm-tag-orange {
  color: #ff9200;
  color: var(--unfilled-text-color, var(--common_orange1_color, #ff9200));
}
.dtm-tag-water {
  color: #5ac8fa;
  color: var(--unfilled-text-color, var(--common_water1_color, #5ac8fa));
}
