.dtm-search-bar {
  display: flex;
  flex-direction: row;
  align-items: center;
  padding: 8px 16px;
  background: #ffffff;
  background: var(--common_bg_z1_color, #ffffff);
}
.dtm-search-bar-wrap {
  display: flex;
  flex: 1;
  flex-direction: row;
  align-items: center;
  height: 36px;
  background: rgba(126, 134, 142, 0.16);
  background: var(--common_line_light_color, rgba(126, 134, 142, 0.16));
  border-radius: var(--common_border_radius_m, 6px);
}
.dtm-search-bar-input {
  flex: 1;
  height: 24px;
  font-size: var(--common_h2_text_style__font-size, 18px);
  line-height: var(--common_h2_text_style__line-height, 1.4);
  padding: 0;
  color: #171a1d;
  color: var(--common_level1_base_color, #171a1d);
  background: none;
}
.dtm-search-bar-input-disabled {
  color: rgba(23, 26, 29, 0.24);
  color: var(--common_level4_base_color, rgba(23, 26, 29, 0.24));
}
.dtm-search-bar .placeholder-input {
  color: var(--common_level3_base_color, rgba(23, 26, 29, 0.4));
}
.dtm-search-bar-icon {
  width: 36px;
  color: rgba(23, 26, 29, 0.4);
  color: var(--common_level3_base_color, rgba(23, 26, 29, 0.4));
  text-align: center;
}
.dtm-search-bar-icon.active {
  color: #171a1d;
  color: var(--common_level1_base_color, #171a1d);
}
.dtm-search-bar-cancel-text {
  font-size: var(--common_h2_text_style__font-size, 18px);
  line-height: var(--common_h2_text_style__line-height, 1.4);
  color: #171a1d;
  color: var(--common_level1_base_color, #171a1d);
  margin-left: 16px;
}
