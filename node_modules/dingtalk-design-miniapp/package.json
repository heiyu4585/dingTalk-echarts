{
  "name": "dingtalk-design-miniapp",
  "version": "1.9.7",
  "description": "DingTalk Design Miniapp 提供统一钉钉风格小程序组件",
  "files": [
    "/lib",
    "/es",
    "/px"
  ],
  "keywords": [
    "dingtalk",
    "dingui",
    "dingui-mini",
    "dingtalk-design-miniapp",
    "dingtalk-design",
    "miniprogram"
  ],
  "scripts": {
    "start": "gulp watch",
    "test-server": "tiny river --miniprogramRoot=./",
    "test-build": "tiny build",
    "test": "jest",
    "build": "gulp build && node scripts/build.js --npm",
    "build-demo:npm": "gulp build && node scripts/build.js --demo --npm",
    "build-openbox": "npm run build-demo:npm && cd demo && tnpm i && cd ../ && node ./scripts/buildBox.js && node ./scripts/removeOpenboxComment.js",
    "prepublishOnly": "npm run build",
    "pub": "npm publish dist",
    "pub:alpha": "npm publish dist --tag alpha",
    "pub:beta": "npm publish dist --tag beta",
    "build:dev": "node scripts/rmes.js && gulp  build && node scripts/cp.js --tnpm --dev && node scripts/rm.js",
    "build:zip": "node scripts/rmes.js && gulp build&& node scripts/cp.js --npm  --dev --zip",
    "add": "node scripts/add.js",
    "update-icon": "node scripts/update-icon.js",
    "update-compI18n": "node scripts/update-compI18n.js",
    "lint": "npm run lint:es && npm run lint:style",
    "lint:es": "eslint -c .eslintrc ./src",
    "lint:style": "stylelint './src/{core,demo}/**/*.less' --syntax less",
    "lint-fix": "eslint -c .eslintrc --fix ./src && stylelint --fix './src/{core,demo}/**/*.less' --syntax less",
    "lint-fix:es": "eslint -c .eslintrc --fix ./src",
    "lint-fix:style": "stylelint --fix './src/{core,demo}/**/*.less' --syntax less",
    "changelog": "node ./scripts/print-changelog",
    "publish:normal": "npm run build && node ./scripts/publish"
  },
  "pre-commit": [
    "lint"
  ],
  "devDependencies": {
    "@ali/aship": "^0.0.1",
    "@ali/dingtalk-medusa": "^10.0.0",
    "@ali/dingui-theme": "^1.0.0",
    "@ali/eslint-config-dingtalk": "^1.62.0",
    "@ali/eslint-plugin-dingtalk": "^0.5.0",
    "@ali/generator-dtest": "^0.0.7",
    "@alipay/openbox-cli": "^1.0.18",
    "@alipay/tiny-cli": "0.60.1-beta.0",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "chalk": "^4.1.0",
    "colors": "^1.4.0",
    "compressing": "^1.4.0",
    "dingtalk-theme": "^6.0.3",
    "eslint": "^5.16.0",
    "fs-extra": "^7.0.0",
    "gulp": "^4.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-cli": "2.1.0",
    "gulp-less": "5.0.0",
    "gulp-remove-empty-lines": "^0.1.0",
    "gulp-rename": "^2.0.0",
    "gulp-strip-css-comments": "^2.0.0",
    "inquirer": "^6.2.2",
    "jest": "^24.8.0",
    "json-beautify": "^1.1.0",
    "ora": "^5.3.0",
    "path": "^0.12.7",
    "postcss": "8.3.5",
    "postcss-transform-unit": "^1.1.2",
    "pre-commit": "^1.2.2",
    "shelljs": "^0.8.1",
    "simple-git": "^3.15.0",
    "stylelint": "^10.0.0",
    "stylelint-config-prettier": "^5.0.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.0.0",
    "yargs": "^13.2.2"
  },
  "tnpm": {
    "mode": "yarn"
  },
  "author": "dingtalk-fe",
  "license": "MIT",
  "dependencies": {
    "fast-deep-equal": "^3.1.3"
  }
}
